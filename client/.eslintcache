   </Form.Group>\n\n            <Form.Group>\n              <div className=\"button_holder\">\n                <Button className=\"send-btn\" size=\"md\" type=\"submit\">\n                  Submit\n                </Button>\n                <Button\n                  className=\"close-btn\"\n                  size=\"md\"\n                  onClick={(e) => {\n                    this.hideAll();\n                  }}\n                >\n                  Close\n                </Button>\n              </div>\n            </Form.Group>\n          </Form>\n        </Modal.Body>\n        <CommentInputError show={this.state.usrinput_error}></CommentInputError>\n        <CommentSendSuccess show={this.state.success}></CommentSendSuccess>\n      </Modal>\n    );\n  }\n\n  addCommentText = (e) => {\n    this.setState({ comment_txt: e.target.value });\n  };\n\n  submitComment = async (e) => {\n    e.preventDefault();\n    const userid = this.props.data.userid;\n    if (userid === undefined || userid === \"\") {\n      this.setState({ usrinput_error: true });\n      return;\n    }\n\n    const schoolid = this.props.data.schoolid;\n\n    const savingResult = await pushCommentToDB(\n      userid,\n      schoolid,\n      this.state.comment_txt\n    );\n\n    const r = await savingResult.new_comment;\n\n    if (savingResult.body !== null) {\n      this.setState({ success: true });\n\n      this.props.incrementQty();\n    }\n\n    window.setTimeout(() => {\n      this.hideAll();\n    }, 2000);\n  };\n\n  hideAll = (e) => {\n    this.props.onClose && this.props.onClose(e);\n    this.setState({ usrinput_error: false });\n    this.setState({ success: false });\n  };\n}\n\nexport default CommentInput;\n","/Users/oismagilova/HYF/iSchool/client/src/components/Comment/CommentsList.js",[],"/Users/oismagilova/HYF/iSchool/client/src/components/Comment/utils/pushCommentToDB.js",[],"/Users/oismagilova/HYF/iSchool/client/src/components/Comment/CommentInputError.js",[],"/Users/oismagilova/HYF/iSc